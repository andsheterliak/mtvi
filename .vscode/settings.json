{
  // --- eslint
  "[javascript]": {
    // Disable editor's format on save.
    "editor.formatOnSave": false,
  },
  // Disable default editor validation to enable eslint validation.
  "javascript.validate.enable": false,
  // To disable editor's fix on save for eslint.
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  // --- cSpell plugin
  // Include words to the dictionary.
  "cSpell.words": [
    "TMDB",
    "mtvi"
  ],
}
