  "homepage": "https://github.com/andsheterliak/mtvi",
  "author": "andsheterliak",
  "private": true,
  "license": "MIT",
  "scripts": {
    "netlify:start": "netlify dev",
    "netlify:preview": "netlify deploy",
    "netlify:deploy": "netlify deploy -p",
